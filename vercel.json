{
    "version": 2,
    "builds": [
        {
            "src": "config/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "config/wsgi.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "DJANGO_SETTINGS_MODULE": "config.settings.production",
        "ALLOWED_HOSTS": ".vercel.app"
    }
}